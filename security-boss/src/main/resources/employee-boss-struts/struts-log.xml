<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="com.zbjdl.boss.admin.log" extends="employee-default" namespace="/log">
		<action name="queryOperationLog" class="com.zbjdl.boss.admin.log.QueryOperationLogAction">
			<result name="success">/WEB-INF/pages/log/queryOperationLog.jsp</result>
		</action>
		<action name="logDetail" class="com.zbjdl.boss.admin.log.QueryOperationLogAction" method="queryLogDetail">
			<result name="success">/WEB-INF/pages/log/logDetail.jsp</result>
		</action>
	</package>
</struts>
