html,body{width:100%;height:100%;}
body{background:#f1f2f7;color:#767676;font-size:12px;font-family: "Microsoft Yahei",Helvetica Neue, Helvetica, Hiragino Sans GB, WenQuanYi Micro Hei, sans-serif;}
.form-control::-moz-placeholder {font-size:12px;}
.form-control:-ms-input-placeholder {font-size:12px;}
.form-control::-webkit-input-placeholder {font-size:12px;}
.btn-default,.btn-primary{font-size:12px;}
.content_blank_10{display:inline-block;width:10px;}
.content_blank_15{display:inline-block;width:15px;}
.display_none{display:none;}
.ml_15{margin-left:15px!important;}
.ml_20{margin-left:20px!important;}
.pt_7{padding-top:7px;}
.align_center{text-align:center;}
.align_right{text-align: right;}
button.submit{background:#1fb5ad;border-color:#1fb5ad;color:#fff;}
button.submit:hover, button.submit:focus, button.submit:active, button.submit.active{background-color: #1ca59e;border-color: #1ca59e;color: #FFFFFF;}
select.select{display:inline-block;width:50px;height:30px;}
table.table{margin:15px auto 15px auto;}
a.in_table{color:#1fb5ad;text-decoration:none;cursor:pointer;}
.nav-tabs>li.active>a, .nav-tabs>li.active>a:focus, .nav-tabs>li.active>a:hover,.nav-tabs>li>a{color:#1fb5ad;}
h2.title{color:#1fb5ad;font-size:16px;margin:15px 0px;}
.pagination{margin:0px auto 15px auto;}
.pagination>.active>a, .pagination>.active>a:focus, .pagination>.active>a:hover, .pagination>.active>span, .pagination>.active>span:focus, .pagination>.active>span:hover{background:#1fb5ad;border-color: #1fb5ad;}
.pagination>li>a, .pagination>li>span{color:#1fb5ad;}
.input-group[class*=col-].plr_15{padding:0 15px;}
.pr{position:relative;}
.panel-default-reset{border-top:none;border-top-left-radius:0px;border-top-right-radius:0px;}
.nav_normal{position:fixed;width:100%;height:80px;background:#fff;box-shadow: 1px 0 3px rgba(0,0,0,.15);}
.nav_normal .logo{width:240px;height:80px;line-height:80px;background:#1fb5ad;text-align:center;}
.wrapper{position:relative;width:100%;height:100%;}
.content_logo{position:absolute;top:0px;left:0px;width:240px;height:80px;line-height:80px;text-align: center;background:#1fb5ad;}
.content_nav{position:absolute;top:0px;left:240px;right:0px;height:80px;padding:23px 20px;background:#fff;box-shadow: 1px 0 3px rgba(0,0,0,.15);min-width: 1040px;}
.content_nav .notice{float:left;position:relative;width:33px;height:33px;margin-right:20px;border-radius:16px;background:#f6f6f6;color:#666666;text-align: center;line-height: 33px;}
.content_nav .notice:hover{background:#1fb5ad;}
.content_nav .notice .count{position:absolute;left:24px;top:-5px;}
.content_nav .notice .badge.bg-success{background:#a9d86e!important;}
.content_nav .notice .badge.bg-info{background:#ff6c60!important;}
.content_nav .notice .badge.bg-warning{background:#fcb322!important;}
.content_nav .notice .badge.bg-danger{background:#428bca!important;}
.content_nav .user{float:right;width:123px;height:33px;border-radius:16px;background:#f6f6f6;}
.content_nav .user .photo{width:33px;height:33px;border-radius:16px;}
.content_nav .user .name{display:inline-block;padding-left:10px;text-align:left;}
.content_nav .quit{float:right;width:33px;height:33px;margin-left:20px;border-radius:16px;background:#f6f6f6;color:#666666;text-align: center;line-height: 33px;}
.content_nav .quit:hover{background:#1fb5ad;}
.content_menu{position:absolute;top:80px;bottom:0px;left:0px;width:240px;background:#32323a;cursor:pointer;overflow:auto;}
.content_menu ul.menu,.content_menu ul.menu li{list-style-type:none;margin:0px;padding:0px;}
.content_menu ul.menu li{border-bottom:1px solid #3c3c43;}
.content_menu ul.menu li a{display:block;width:240px;height:54px;padding:0 20px;line-height:54px;color:#aeb2b7;font-size:12px;text-decoration: none;}
.content_menu ul.menu li a .arr_right{float:right;line-height: 54px;}
.content_menu ul.menu li a:hover{color:#1fb5ad;background:#28282e;}
.content_menu ul.menu li a.active{background:#28282e;color:#1fb5ad;}
.content_menu ul.menu_active,.content_menu ul.menu_active li{list-style-type: none;margin:0px;padding:0px;border:none;}
.content_menu ul.menu_active li a{display:block;background:#28282e;border-bottom:none;font-size:12px;padding-left:45px;}
.content_menu ul.menu_active li a:hover{color:#1fb5ad;background:#1f1f24;}
.content_menu ul.menu_active li a.selected{background:#1f1f24;color:#1fb5ad;}
.content_wrapper{position:absolute;top:80px;bottom:0px;left:240px;right:0px;min-width: 1040px;}
.content_main{padding:20px;}
.content_main form a.select{display:inline-block;padding:7px 10px;color:#767676;text-decoration: none;border-radius:2px;cursor:pointer;margin-right:10px;}
.content_main form a.select:hover{background:#f5f5f5;}
.content_main form a.select.active{color:#fff;background:#999999;}
.content_main form .selectDepart{border-bottom:1px dashed #ddd;padding-bottom:15px;}
.darkBg{width:100%;height:100%;background:#32323A;}
.greenBg{width:100%;height:100%;background:#35bcb5;}
.form_outer{width:1000px;padding:100px 0px 0px;margin:0 auto;}
.form_size{width:320px;margin:0px auto;border-radius:5px;background:#eaeaec;}
.form_title{width:320px;height:90px;line-height:90px;text-align:center;font-size:16px;color:#fff;background:#aec785;border-bottom:10px solid #a0b87e;border-top-left-radius: 5px;border-top-right-radius: 5px;}
.form_team{padding:20px 20px 0px;position:relative;}
.input_content{width:280px;height:35px;margin-bottom:20px;border:1px solid #c2c2c2;border-radius: 5px;background:#fff;position:relative;}
.input_content .input_label{position:absolute;display:block;width:40px;height:33px;line-height: 33px;top:0px;left:0px;text-align:center;margin:0px;}
.input_content .input_self{width:230px;height:33px;border:0px;position:absolute;left:40px;top:0px;}
.input_content_s{width:180px;height:35px;margin-bottom:20px;border:1px solid #c2c2c2;border-radius: 5px;background:#fff;position:relative;}
.input_content_s .input_label{position:absolute;display:block;width:40px;height:33px;line-height: 33px;top:0px;left:0px;text-align:center;margin:0px;}
.input_content_s .input_self{width:130px;height:33px;border:0px;position:absolute;left:40px;top:0px;}
.input_content_s img{position:absolute;top:0px;left:198px;width:80px;height:33px;}
.form_submit{padding:20px;background:#fff;border-bottom-left-radius: 5px;border-bottom-right-radius:5px;}
.submit_login{width:280px;height:38px;background:#1FB5AD;font-size:14px;color:#fff;border:0px;border-radius:5px;}
.blank_20{display:inline-block;width:20px;}
.tips_head{width:100%;height:280px;background:#fff;}
.tips_body{width:100%;height:150px;background:#1fb5ad;position:relative;text-align:center;}
.tips_body .title{width:100%;position:absolute;top:-80px;font-size:45px;color:#1fb5ad;}
.tips_body .detail{width:100%;color:#fff;font-size:20px;line-height:150px;}
.tips_foot{width:100%;height:200px;color:#fff;text-align:center;}
.tips_foot a{margin-top:80px;display:inline-block;width:150px;height:45px;line-height:45px;border:1px solid #fff;border-radius:5px;font-size:14px;color:#fff;}
@font-face {font-family: "iconfont";
  src: url('../fonts/iconfont.eot'); /* IE9*/
  src: url('../fonts/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('../fonts/iconfont.woff') format('woff'), /* chrome, firefox */
  url('../fonts/iconfont.ttf') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('../fonts/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:20px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.icon-p-password:before { content: "\e67a"; }
.icon-yonghuming:before { content: "\e624"; }
.icon-yanzhengma:before { content: "\e608"; }
